encoding "utf8";

import "kwtypes.proto";

action "купить_слово" {
	key = 'купить' | 'куп' | 'к' | 'buy' | 'b' | 'купля' | 'покупка' | 'покуп' | 'приобретать';
	lemma = "buy";
}

action "ломбард_слово" {
	key = 'ломбард' | 'ломб' | 'л' | 'lomb' | 'lombard' | 'pawn';
	lemma = "pawn";
}

action "продать_слово" {
	key = 'продать' | 'прод' | 'продажа' | 'продает' | 'продаёт' | 'sell' | 's';
	lemma = "sell";
}
